doctype 5
html(data-ng-app="TPCv4")
    head
        title= title
        link(rel="stylesheet", href="/stylesheets/style.css")
        link(rel="stylesheet", href="/javascripts/vendor/bootstrap/dist/css/bootstrap.min.css")
        link(rel="stylesheet", href="/javascripts/vendor/bootstrap/dist/css/bootstrap-theme.min.css")
        link(rel="stylesheet", href="/javascripts/vendor/bootstrap/examples/carousel/carousel.css")
        script(type="text/javascript", src='/javascripts/vendor/bootstrap/docs-assets/js/holder.js')
        script(type="text/javascript", src="/javascripts/vendor/jquery/jquery.min.js")
        script(type="text/javascript", src="/javascripts/vendor/bootstrap/dist/js/bootstrap.min.js")
        script(type="text/javascript", src="/javascripts/vendor/angular/angular.min.js")
        script(type="text/javascript", src="/javascripts/vendor/angular-route/angular-route.min.js")
        script(type="text/javascript", src="/javascripts/app.js")
